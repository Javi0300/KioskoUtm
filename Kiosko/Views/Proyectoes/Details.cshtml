@model Kiosko.Models.Proyecto

@{
    ViewData["Title"] = "Details";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<br />
<br />
<br />
<br />
<br />
<br />
<br />
<h1>Details</h1>

<div>
    <h4>Proyecto</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            NOMBRE DEL PROYECTO
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Nombreproyecto)
        </dd>
        <dt class="col-sm-2">
            video DEL PROYECTO
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Videoproyecto)
        </dd>
        <dt class="col-sm-2">
            DESCRIPCION DEL PROYECTO
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Descripcionproyecto)
        </dd>
        <dt class="col-sm-2">
            FOTO
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Fotoproyecto)
        </dd>
        <dt class="col-sm-2">
           DIVISION
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.IddivisionesNavigation.Nombredivision)
        </dd>
    </dl>
</div>
<div>
    <a class="btn btn-outline-primary" asp-action="Edit" asp-route-id="@Model.Idproyectos">EDITAR</a> |
    <a class="btn btn-outline-primary" asp-action="Index">REGRESAR</a>
</div>
