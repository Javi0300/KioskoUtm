@model Kiosko.Models.Proyecto

@{
    ViewData["Title"] = "Delete";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<br />
<br />
<br />
<br />
<br />
<br />
<br />
<h1>Delete</h1>

<h3>¿SEGURO QUE DESEAS BORRAR ESTE REGISTRO?</h3>
<div>
    <h4>Proyecto</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            NOMBRE DEL PROYECTO
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Nombreproyecto)
        </dd>
        <dt class="col-sm-2">
            VIDEO
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Videoproyecto)
        </dd>
        <dt class="col-sm-2">
            DESCRIPCION
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Descripcionproyecto)
        </dd>
        <dt class="col-sm-2">
           FOTO
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Fotoproyecto)
        </dd>
        <dt class="col-sm-2">
           DIVISION
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.IddivisionesNavigation.Nombredivision)
        </dd>
    </dl>

    <form asp-action="Delete">
        <input type="hidden" asp-for="Idproyectos" />
        <input type="submit" value="Delete" class="btn btn-danger" /> |
        <a class="btn btn-danger" asp-action="Index">REGRESAR</a>
    </form>
</div>
